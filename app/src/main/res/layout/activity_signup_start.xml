<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignupStartActivity"
    android:background="@drawable/custom_7718">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="128dp"
        android:fontFamily="@font/noto_sans_cjkkr_bold"
        android:gravity="end"
        android:letterSpacing="-0.05"
        android:lineSpacingExtra="22.5sp"
        android:shadowColor="#009a0f0f"
        android:shadowDx="0"
        android:shadowDy="3"
        android:shadowRadius="6"
        android:textColor="#6343e6"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="시작하기" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:fontFamily="@font/noto_sans_cjkkr_regular"
        android:gravity="end"
        android:letterSpacing="-0.05"
        android:lineSpacingExtra="15sp"
        android:textColor="#000000"
        android:textSize="33sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.782"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:text="명함을\n만들고 활동을\n시작하세요" />

    <FrameLayout
        android:layout_width="265dp"
        android:layout_height="47dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.904">

        <Button
            android:id="@+id/view2"
            android:layout_width="267dp"
            android:layout_height="match_parent"
            android:background="@drawable/custom_24018"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            tools:visibility="invisible"></Button>

        <TextView
            android:layout_width="45dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:fontFamily="@font/noto_sans_cjkkr_regular"
            android:gravity="end"
            android:letterSpacing="-0.05"
            android:lineSpacingExtra="9sp"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="normal"
            app:layout_constraintBottom_toBottomOf="@+id/view2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/view2"
            tools:text="GO!"
            tools:visibility="visible"/>

    </FrameLayout>






</androidx.constraintlayout.widget.ConstraintLayout>